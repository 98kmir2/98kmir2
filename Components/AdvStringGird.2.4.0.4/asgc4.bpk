!if !$d(BCB)
BCB = $(MAKEDIR)\..
!endif

# ---------------------------------------------------------------------------
# IDE SECTION
# ---------------------------------------------------------------------------
# The following section of the project makefile is managed by the BCB IDE.
# It is recommended to use the IDE to change any of the values in this
# section.
# ---------------------------------------------------------------------------

VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = asgc4.bpl
OBJFILES = asgreg.obj asgprnr.obj asgprvr.obj asghtmlr.obj asgfindr.obj \
  PictureContainerReg.obj asgreplacer.obj asgc4.obj
RESFILES = asgc4.res asgreg.dcr asgprnr.dcr asgprvr.dcr asghtmlr.dcr asgfindr.dcr \
  PictureContainerReg.dcr asgreplacer.dcr
RESDEPEN = $(RESFILES)
LIBFILES =
LIBRARIES = dclocx40.lib nmfast40.lib \
  tee40.lib teedb40.lib teeui40.lib vcldbx40.lib ibsmp40.lib vcldb40.lib \
  qrpt40.lib bcbsmp40.lib vcljpg40.lib vclx40.lib
SPARELIBS = Vcl40.lib vclx40.lib vcljpg40.lib bcbsmp40.lib qrpt40.lib vcldb40.lib \
  ibsmp40.lib vcldbx40.lib teeui40.lib teedb40.lib tee40.lib nmfast40.lib \
  dclocx40.lib
PACKAGES = vcl40.bpi
DEFFILE =
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
USERDEFINES =
SYSDEFINES = _RTLDLL;NO_STRICT;USEPACKAGES
# ---------------------------------------------------------------------------
CFLAG1 = -I$(BCB)\comp;$(BCB)\include;$(BCB)\include\vcl -Od -Hc -H=$(BCB)\lib\vcl40.csm \
  -w -Ve -r- -a8 -k -y -v -vi- -c -b- -w-par -w-inl -Vx -tWM \
  -D$(SYSDEFINES);$(USERDEFINES)
PFLAGS = -U$(BCB)\Projects\Lib;$(BCB)\comp;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I$(BCB)\comp;$(BCB)\include;$(BCB)\include\vcl -$YD -$W -$O- -v -JPHNE -M
RFLAGS = -i$(BCB)\comp;$(BCB)\include;$(BCB)\include\vcl
AFLAGS = /i$(BCB)\comp /i$(BCB)\include /i$(BCB)\include\vcl /mx /w2 /zd
LFLAGS = -L$(BCB)\Projects\Lib;$(BCB)\comp;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -D"TMS software TAdvStringGrid" -aa -Tpp -x -Gn -Gl -Gi -v
# ---------------------------------------------------------------------------
ALLOBJ = c0pkg32.obj Memmgr.Lib $(PACKAGES) sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mti.lib
# ---------------------------------------------------------------------------
