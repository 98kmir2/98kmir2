VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = VCLZipBCB4_3.bpl
OBJFILES = kpSFXCfg.obj VCLUnZip.obj VCLZip.obj VCLZipBCB4_3.obj
RESFILES = VCLZipBCB4_3.res kpSFXCfg.dcr VCLUnZip.dcr VCLZip.dcr
RESDEPEN = $(RESFILES)
LIBFILES =
LIBRARIES = dclocx40.lib nmfast40.lib tee40.lib teedb40.lib teeui40.lib vcldbx40.lib \
  ibsmp40.lib vcldb40.lib qrpt40.lib bcbsmp40.lib vcljpg40.lib
SPARELIBS = Vcl40.lib vclx40.lib vcljpg40.lib bcbsmp40.lib qrpt40.lib vcldb40.lib \
  ibsmp40.lib vcldbx40.lib teeui40.lib teedb40.lib tee40.lib nmfast40.lib \
  dclocx40.lib
PACKAGES = vcl40.bpi vclx40.bpi
DEFFILE =
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
USERDEFINES =
SYSDEFINES = _RTLDLL;NO_STRICT;USEPACKAGES
# ---------------------------------------------------------------------------
CFLAG1 = -I$(BCB)\include;$(BCB)\include\vcl -O2 -Hc -H=$(BCB)\lib\vcl40.csm -w -Ve -a8 \
  -k- -vi -c -b- -w-par -w-inl -Vx -tWM -D$(SYSDEFINES);$(USERDEFINES)
PFLAGS = -U..\..\..\Borland\CBuilder4\Lib\Release;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I$(BCB)\include;$(BCB)\include\vcl -$Y- -$L- -$D- -v -JPHNE -M
RFLAGS = -i$(BCB)\include;$(BCB)\include\vcl
AFLAGS = /i$(BCB)\include /i$(BCB)\include\vcl /mx /w2 /zn
LFLAGS = -L..\..\..\Borland\CBuilder4\Lib\Release;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -D"VCLZip Pro" -aa -Tpp -x -Gn -Gl -Gi
# ---------------------------------------------------------------------------
ALLOBJ = c0pkg32.obj Memmgr.Lib $(PACKAGES) sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mti.lib
